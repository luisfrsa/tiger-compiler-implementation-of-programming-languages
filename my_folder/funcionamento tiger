parse_program
	parse_statement_seq
	while !done
		parse_statement() [append]
		(let var a:int:=1)


Tree Parser::parse_expression
	Tree expr -> Tree build_tree(integer_type_node,atoi (tok->get_str ().c_str ())),tok->get_locus ());

Tree Parser::parse_statement 
	TreeStmtList stmt_list.append().get_tree();

inicia programa:
	enquanto houver expressões faça:
		switch case expressão
			caso expressão numerica
			caso expressão string
			caso let, if, while, for...



var x = 
let
	var a=0
	var b=0
in
	a:= if 1>2 then 10 else 20;
	while a>10
	do
		write a;
		a:=a+a;
	end
	b:=if a>b
	then
		a++
		1
	end
	else
		0
	end
	1
end
write x